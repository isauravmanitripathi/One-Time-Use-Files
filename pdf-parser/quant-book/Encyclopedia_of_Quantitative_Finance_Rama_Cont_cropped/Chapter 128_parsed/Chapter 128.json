        },
        {
          "id": "/page/1/SectionHeader/5",
          "block_type": "SectionHeader",
          "html": "<h3><b>EDS Pricing</b></h3>",
          "polygon": [
            [
              79.05859375,
              363.733642578125
            ],
            [
              141.49609375,
              363.733642578125
            ],
            [
              141.49609375,
