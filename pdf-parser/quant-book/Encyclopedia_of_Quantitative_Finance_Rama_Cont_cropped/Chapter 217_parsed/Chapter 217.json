        {
          "id": "/page/0/SectionHeader/0",
          "block_type": "SectionHeader",
          "html": "<h2><b>Eurodollar Futures and </b>Options</h2>",
          "polygon": [
            [
              74.1873417721519,
              70.45328031809146
            ],
            [
              264.63671875,
              70.45328031809146
            ],
            [
              264.63671875,
              109.252685546875
            ],
            [
              74.1873417721519,
              109.252685546875
            ]
          ],
          "bbox": [
            74.1873417721519,
            70.45328031809146,
            264.63671875,
            109.252685546875
          ],
          "children": null,
          "section_hierarchy": {
