        {
          "id": "/page/0/SectionHeader/0",
          "block_type": "SectionHeader",
          "html": "<h2><b>Free Lunch </b></h2>",
          "polygon": [
            [
              73.4379746835443,
              70.45328031809146
            ],
            [
              169.35696202531645,
              70.45328031809146
            ],
            [
              169.35696202531645,
              88.6815185546875
            ],
            [
              73.4379746835443,
              88.6815185546875
            ]
          ],
          "bbox": [
            73.4379746835443,
            70.45328031809146,
            169.35696202531645,
