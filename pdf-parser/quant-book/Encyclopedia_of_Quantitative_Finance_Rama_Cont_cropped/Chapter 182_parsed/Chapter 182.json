        {
          "id": "/page/0/ListGroup/284",
          "block_type": "ListGroup",
          "html": "<p block-type='ListGroup'><ul><content-ref src='/page/0/ListItem/14'></content-ref><content-ref src='/page/0/ListItem/15'></content-ref><content-ref src='/page/0/ListItem/16'></content-ref></ul></p>",
          "polygon": [
            [
              303.1199951171875,
              464.02569580078125
            ],
            [
              513.953125,
              464.02569580078125
            ],
