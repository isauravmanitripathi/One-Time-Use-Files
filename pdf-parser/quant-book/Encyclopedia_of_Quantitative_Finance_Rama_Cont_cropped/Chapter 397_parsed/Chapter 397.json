        {
          "id": "/page/0/SectionHeader/0",
          "block_type": "SectionHeader",
          "html": "<h2><b>Phase-type Distribution </b></h2>",
          "polygon": [
            [
              73.4379746835443,
              71.95228628230616
            ],
            [
              263.0278481012658,
              71.95228628230616
            ],
            [
              263.0278481012658,
              87.623046875
            ],
            [
              73.4379746835443,
              87.623046875
            ]
          ],
          "bbox": [
            73.4379746835443,
            71.95228628230616,
            263.0278481012658,
            87.623046875
          ],
          "children": null,
          "section_hierarchy": {
