        {
          "id": "/page/0/SectionHeader/0",
          "block_type": "SectionHeader",
          "html": "<h1><b>Duration Models </b></h1>",
          "polygon": [
            [
              73.4379746835443,
              71.2027833001988
            ],
            [
              209.8227848101266,
              71.2027833001988
            ],
            [
              209.8227848101266,
              88.2213134765625
            ],
            [
              73.4379746835443,
              88.2213134765625
            ]
          ],
          "bbox": [
            73.4379746835443,
            71.2027833001988,
            209.8227848101266,
