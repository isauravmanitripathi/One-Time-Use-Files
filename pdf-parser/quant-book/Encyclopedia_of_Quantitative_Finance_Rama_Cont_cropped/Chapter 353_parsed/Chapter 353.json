        {
          "id": "/page/1/SectionHeader/2",
          "block_type": "SectionHeader",
          "html": "<h2><b>Proportional Hazard and Survival Models</b></h2>",
          "polygon": [
            [
              78.986328125,
              119.377197265625
            ],
            [
              285.8828125,
              119.377197265625
            ],
            [
              285.8828125,
              130.921630859375
            ],
            [
              78.986328125,
              130.921630859375
            ]
          ],
          "bbox": [
            78.986328125,
            119.377197265625,
            285.8828125,
            130.921630859375
          ],
          "children": null,
          "section_hierarchy": {
