        {
          "id": "/page/0/SectionHeader/4",
          "block_type": "SectionHeader",
          "html": "<h4>Scale Models</h4>",
          "polygon": [
            [
              74.9367088607595,
              554.455078125
            ],
            [
              140.13164556962025,
              554.455078125
            ],
            [
              140.13164556962025,
              564.763671875
            ],
            [
              74.9367088607595,
              564.763671875
            ]
          ],
          "bbox": [
            74.9367088607595,
            554.455078125,
            140.13164556962025,
            564.763671875
          ],
          "children": null,
