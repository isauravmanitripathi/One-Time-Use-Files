    {
      "id": "/page/0/SectionHeader/7",
      "block_type": "SectionHeader",
      "html": "<h2><b>Managed CDOs Have an Arbitrage</b> <b>Proposition</b></h2>",
      "page": 304,
      "polygon": [
        [
          302.9375,
          296.29364013671875
        ],
        [
          476.1199035644531,
          296.29364013671875
        ],
        [
          476.1199035644531,
          320.46142578125
        ],
        [
          302.9375,
          320.46142578125
        ]
      ],
      "bbox": [
        302.9375,
        296.29364013671875,
        476.1199035644531,
        320.46142578125
