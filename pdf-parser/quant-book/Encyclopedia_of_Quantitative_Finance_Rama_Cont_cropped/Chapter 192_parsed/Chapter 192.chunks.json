    {
      "id": "/page/0/SectionHeader/3",
      "block_type": "SectionHeader",
      "html": "<h2><b>Types of Managed CDOs</b></h2>",
      "page": 304,
      "polygon": [
        [
          75.373046875,
          269.6536560058594
        ],
        [
          199.7421875,
          269.6536560058594
