      "children": [
        {
          "id": "/page/0/SectionHeader/0",
          "block_type": "SectionHeader",
          "html": "<h1><b>Cliquet Options </b></h1>",
          "polygon": [
            [
              74.9367088607595,
              70.9176025390625
            ],
            [
              201.57974683544305,
              70.9176025390625
            ],
            [
              201.57974683544305,
              89.4178466796875
            ],
            [
              74.9367088607595,
              89.4178466796875
            ]
          ],
          "bbox": [
            74.9367088607595,
            70.9176025390625,
            201.57974683544305,
