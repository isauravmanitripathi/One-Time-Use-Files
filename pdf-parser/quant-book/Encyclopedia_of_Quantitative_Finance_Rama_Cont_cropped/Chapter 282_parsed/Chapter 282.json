        {
          "id": "/page/0/Equation/9",
          "block_type": "Equation",
          "html": "<p block-type=\"Equation\"><math display=\"block\">\\frac{\\mathrm{E}[R_p] - R_f}{\\sigma[R_p]} \\tag{3}</math></p>",
          "polygon": [
            [
              383.64093017578125,
              111.4652099609375
            ],
            [
              512.796875,
              111.4652099609375
            ],
            [
              512.796875,
              136.314208984375
            ],
            [
              383.64093017578125,
              136.314208984375
            ]
          ],
          "bbox": [
            383.64093017578125,
            111.4652099609375,
            512.796875,
            136.314208984375
          ],
          "children": null,
          "section_hierarchy": {
