        {
          "id": "/page/0/SectionHeader/0",
          "block_type": "SectionHeader",
          "html": "<h2><b>Ross, Stephen</b></h2>",
          "polygon": [
            [
              75.01171875,
              70.9412841796875
            ],
            [
              185.55624389648438,
              70.9412841796875
            ],
            [
              185.55624389648438,
              89.12310791015625
            ],
            [
              75.01171875,
