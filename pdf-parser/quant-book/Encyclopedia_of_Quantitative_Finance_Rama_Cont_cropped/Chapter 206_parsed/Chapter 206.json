        {
          "id": "/page/1/SectionHeader/9",
          "block_type": "SectionHeader",
          "html": "<h4><b>Calibrating to the Correlation Market</b></h4>",
          "polygon": [
            [
              305.828125,
              255.85366821289062
            ],
            [
              494.296875,
              255.85366821289062
            ],
            [
              494.296875,
              267.0616455078125
            ],
            [
              305.828125,
              267.0616455078125
            ]
          ],
          "bbox": [
            305.828125,
            255.85366821289062,
            494.296875,
            267.0616455078125
          ],
          "children": null,
          "section_hierarchy": {
