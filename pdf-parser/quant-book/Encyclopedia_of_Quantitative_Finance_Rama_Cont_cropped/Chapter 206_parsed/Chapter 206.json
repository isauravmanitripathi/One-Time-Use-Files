        {
          "id": "/page/0/SectionHeader/0",
          "block_type": "SectionHeader",
          "html": "<h1><b>Intensity Gamma Model</b></h1>",
          "polygon": [
            [
              75.30078125,
              70.9412841796875
            ],
            [
              268.8379821777344,
              70.9412841796875
            ],
            [
              268.8379821777344,
              89.12310791015625
            ],
            [
              75.30078125,
              89.12310791015625
            ]
          ],
          "bbox": [
            75.30078125,
            70.9412841796875,
            268.8379821777344,
            89.12310791015625
          ],
          "children": null,
          "section_hierarchy": {
