        {
          "id": "/page/0/SectionHeader/4",
          "block_type": "SectionHeader",
          "html": "<h4><b>Model Definition</b></h4>",
          "polygon": [
            [
              75.30078125,
              601.9935302734375
            ],
            [
              157.52957153320312,
              601.9935302734375
            ],
            [
              157.52957153320312,
              613.2015380859375
            ],
            [
              75.30078125,
              613.2015380859375
            ]
          ],
          "bbox": [
