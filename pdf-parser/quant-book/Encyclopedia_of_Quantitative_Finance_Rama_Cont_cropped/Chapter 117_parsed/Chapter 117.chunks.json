    {
      "id": "/page/0/Text/13",
      "block_type": "Text",
      "html": "<p block-type=\"Text\">There are mainly two types of realized correlation formulas currently found on over-the-counter (OTC) markets:</p>",
      "page": 182,
      "polygon": [
        [
          301.24556962025315,
          91.43936381709742
        ],
        [
          513.3164556962025,
          91.43936381709742
        ],
        [
          513.3164556962025,
          124.8076171875
        ],
        [
          301.24556962025315,
          124.8076171875
        ]
      ],
      "bbox": [
        301.24556962025315,
        91.43936381709742,
        513.3164556962025,
        124.8076171875
