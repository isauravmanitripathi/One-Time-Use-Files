    {
      "id": "/page/2/Page/193",
      "block_type": "Page",
      "html": "<content-ref src='/page/2/Text/0'></content-ref><content-ref src='/page/2/Text/1'></content-ref><content-ref src='/page/2/Text/2'></content-ref><content-ref src='/page/2/Text/3'></content-ref><content-ref src='/page/2/Text/4'></content-ref><content-ref src='/page/2/Text/5'></content-ref><content-ref src='/page/2/Text/6'></content-ref><content-ref src='/page/2/Text/7'></content-ref><content-ref src='/page/2/Text/8'></content-ref><content-ref src='/page/2/Text/9'></content-ref><content-ref src='/page/2/Text/10'></content-ref><content-ref src='/page/2/Text/11'></content-ref><content-ref src='/page/2/Text/12'></content-ref><content-ref src='/page/2/PageFooter/13'></content-ref>",
      "polygon": [
        [
          0.0,
          0.0
        ],
        [
          612.0,
          0.0
        ],
        [
          612.0,
          792.0
