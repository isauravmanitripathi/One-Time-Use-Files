    {
      "id": "/page/1/Text/2",
      "block_type": "Text",
      "html": "<p block-type=\"Text\">[26<i>th September</i>, 2020.]</p>",
      "page": 191,
      "polygon": [
        [
          423.71875,
          111.930908203125
        ],
        [
          525.57421875,
          111.930908203125
        ],
        [
          525.57421875,
          123.814208984375
        ],
        [
